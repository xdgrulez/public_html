@Misc{vo,
  author =       {Ralph Debusmann},
  title =        {Eine modellorientierte Erklaerung fuer das Verbal Overshadowing-Phaenomen},
  year =         1999,
  authorURLs =   {http://www.ps.uni-sb.de/~rade/},
  abstract =     {In der Allgemeinen Psychologie herrscht in der Regel die
  Annahme, dass sich Behaltensleistungen durch Versprachlichung der zu
  lernenden Reize erhoehen lassen. In diesem Zusammenhang verwendete
  Techniken, u.a. Verbal Rehearsal und Verbal Elaboration, sind
  effektive Mittel zur Behaltensverbesserung (z.B. Darley/Glass 1975,
  Maki/Schuler 1980). Der Erfolg der Verbalisierung haengt jedoch auch
  vom zu behaltenden Material ab. Gerade beim Behalten perzeptueller
  Reize kann Verbalisierung die Behaltensleistung unter Umstaenden
  sogar verschlechtern. Man spricht dann von Verbal Overshadowing. Im
  vorliegenden Papier beschreiben wir zunaechst eine Studie von
  Melcher/Schooler 1996, die das Paradigma des Verbal Overshadowing
  anschaulich macht. Hiernach setzen wir uns mit den Folgerungen von
  Melcher/Schooler 1996 zu den Ergebnissen ihrer Studie auseinander,
  und setzen diesen im Rahmen des multimodalen Modells von
  Rummer/Engelkamp 1998 dann eine alternative, modellorientierte
  Interpretation entgegen.},
  note =         {Hausarbeit},
}
